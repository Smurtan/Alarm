QFrame#circle_bg {
    border-radius: 310px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0.0 #3986ce, stop: 1.0 #fffa66);
}

QPushButton#alarm_clock {
    border-radius: 50%;
}

QLabel#alarm_clock_time {
    color: #ffffff;
}

QLabel#setting_label {
    color: #ffffff;
}

QLabel#time_label {
    color: #ffffff;
}

QFrame#alarm_clocks_area {
    background: none;
    border: none;
}

QScrollArea#alarm_clocks_scroll_area {
    border: none;
    background-color: rgba(0, 0, 0, 0);
}

QFrame#alarm_clock_setting {
    border-radius: 50%;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #063f73, stop: 1.0 #bf1dc5)
}

QComboBox {
    border-radius: 8px;
    combobox-popup: 0;
    background-color: #9d3cd0;
}

QComboBox::drop-down {
    border: none;
}

QComboBox::drop-down:hover {
    border: none;
    background-color: #5a248e;
    border-bottom-right-radius: 8px;
    border-top-right-radius: 8px;
}

QComboBox::down-arrow {
    image: url(body/image/day/down_arrow.svg);
}

QComboBox QAbstractItemView {
    background-color: #d4b9ef;
}

QComboBox QAbstractSlider {
    background-color: #d4b9ef;
}

QTimeEdit {
    border-radius: 8px;
    border: none;
    background-color: #9d3cd0;
}

QTimeEdit::down-button {
    border: none;
    border-bottom-right-radius: 8px;
    width: 18px;
}

QTimeEdit::down-button:hover {
    border: none;
    background-color: #7f1bbd;
}

QTimeEdit::down-arrow {
    image: url(body/image/day/down_arrow.svg);
}

QTimeEdit::up-button {
    border: none;
    border-top-right-radius: 8px;
    width: 18px;
}

QTimeEdit::up-button:hover {
    border: none;
    background-color: #5a248e;
}

QTimeEdit::up-arrow {
    image: url(body/image/day/up_arrow.svg);
}
